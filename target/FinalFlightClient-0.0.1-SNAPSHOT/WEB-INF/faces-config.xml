<?xml version="1.0"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <navigation-rule>
  <from-view-id>/welcome/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>signup</from-outcome>
   <to-view-id>/welcome/Signup.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/secure/SearchPage.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/welcome/Signup.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/welcome/login.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/secure/SearchPage.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>edit</from-outcome>
   <to-view-id>/secure/EditProfile.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>logout</from-outcome>
   <to-view-id>/welcome/login.xhtml</to-view-id>
   <redirect></redirect>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/secure/EditProfile.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/secure/SearchPage.xhtml</to-view-id>
   <redirect></redirect>
  </navigation-case>
 </navigation-rule>
 <application>
  <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
 </application>
</faces-config>
